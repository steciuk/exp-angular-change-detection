{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"default-cd": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss",
					"skipTests": true
				},
				"@schematics/angular:application": {
					"strict": true
				},
				"@schematics/angular:service": {
					"skipTests": true
				}
			},
			"root": "projects/default-cd",
			"sourceRoot": "projects/default-cd/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/default-cd",
						"index": "projects/default-cd/src/index.html",
						"main": "projects/default-cd/src/main.ts",
						"polyfills": "projects/default-cd/src/polyfills.ts",
						"tsConfig": "projects/default-cd/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": ["projects/default-cd/src/favicon.ico", "projects/default-cd/src/assets"],
						"styles": ["projects/default-cd/src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "projects/default-cd/src/environments/environment.ts",
									"with": "projects/default-cd/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "default-cd:build:production"
						},
						"development": {
							"browserTarget": "default-cd:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "default-cd:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/default-cd/src/test.ts",
						"polyfills": "projects/default-cd/src/polyfills.ts",
						"tsConfig": "projects/default-cd/tsconfig.spec.json",
						"karmaConfig": "projects/default-cd/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": ["projects/default-cd/src/favicon.ico", "projects/default-cd/src/assets"],
						"styles": ["projects/default-cd/src/styles.scss"],
						"scripts": []
					}
				}
			}
		},
		"onpush-cd": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss",
					"skipTests": true
				},
				"@schematics/angular:application": {
					"strict": true
				},
				"@schematics/angular:service": {
					"skipTests": true
				}
			},
			"root": "projects/onpush-cd",
			"sourceRoot": "projects/onpush-cd/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/onpush-cd",
						"index": "projects/onpush-cd/src/index.html",
						"main": "projects/onpush-cd/src/main.ts",
						"polyfills": "projects/onpush-cd/src/polyfills.ts",
						"tsConfig": "projects/onpush-cd/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": ["projects/onpush-cd/src/favicon.ico", "projects/onpush-cd/src/assets"],
						"styles": ["projects/onpush-cd/src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "projects/onpush-cd/src/environments/environment.ts",
									"with": "projects/onpush-cd/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "onpush-cd:build:production"
						},
						"development": {
							"browserTarget": "onpush-cd:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "onpush-cd:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "projects/onpush-cd/src/test.ts",
						"polyfills": "projects/onpush-cd/src/polyfills.ts",
						"tsConfig": "projects/onpush-cd/tsconfig.spec.json",
						"karmaConfig": "projects/onpush-cd/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": ["projects/onpush-cd/src/favicon.ico", "projects/onpush-cd/src/assets"],
						"styles": ["projects/onpush-cd/src/styles.scss"],
						"scripts": []
					}
				}
			}
		}
	},
	"defaultProject": "default-cd"
}
